package  {		import fl.transitions.Tween	import fl.transitions.easing.*	import flash.display.MovieClip;	import flash.events.Event;	public class Pipe extends MovieClip	{				public var mcLiqBase:MovieClip		public var mcFlow:MovieClip				public var animRunning:Boolean = false		public var flowRate:Number = 0		public var beginX:Number 		public var beginY:Number 		public var endX:Number		public var endY:Number 		public var xT:Tween 		public var yT:Tween				public function Pipe() 		{				}		 				/* Function setFlow:		*  Set speed of animation 		*  Parameters:		*  intvSecs - interval for animation in seconds		*/				public function setFlow(intvSecs:Number)		{			if (intvSecs <= 0 ) 			{				if (animRunning) stopAnim()			}			else			{				flowRate = intvSecs				if (animRunning==false)				{					startAnim()				}				else				{					xT.resume()					yT.resume()				}				xT.duration = intvSecs				yT.duration = intvSecs			}								}						protected function startAnim()		{			animRunning = true						if (xT==null)			{				xT = new Tween(mcFlow, "x", None.easeNone, beginX, endX, 5, true)				yT = new Tween(mcFlow, "y", None.easeNone, beginY, endY, 5, true)				xT.looping = true				yT.looping = true			}			else			{				xT.stop()				yT.stop()			}					if (flowRate <= 0 ) return			xT.duration = flowRate			yT.duration = flowRate			xT.start()			yT.start()		}				protected function stopAnim()		{			animRunning = false				if (xT)			{				xT.stop()				yT.stop()							}		}									}	}